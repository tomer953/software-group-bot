{
  "name": "software-group-bot",
  "version": "1.0.3",
  "description": "Telegram bot for our software group",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "start": "node ./dist/index.js",
    "prettier:write": "prettier --write \"./**/*.{,ts}\"",
    "prettier:check": "prettier --list-different \"./**/*.{,ts}\"",
    "eslint:check": "eslint --fix-dry-run \"./**/*.{,ts}\"",
    "eslint:write": "eslint --fix \"./**/*.{,ts}\"",
    "precommit": "lint-staged"
  },
  "author": "tomer953",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.2.0",
    "cheerio": "^1.0.0-rc.12",
    "common-tags": "^1.8.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.7.4",
    "node-html-parser": "^6.1.4",
    "node-schedule": "^2.1.0",
    "nodemon": "^2.0.22",
    "ntfm": "^0.0.12",
    "telegraf": "^4.11.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.195",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.18.4",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "telegram-typings": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
